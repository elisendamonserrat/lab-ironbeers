<div>
  {{#each randomBeer}}
    <img src="{{image_url}}" alt="{{tagline}}">
    <h2>{{name}}</h2>
        <p><span>{{tagline}}</span></p>
        <p>{{description}}</p>
    <h3>Food Pairing</h3>
        <ul>
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    <p><strong>Brewer Tips:</strong> {{brewers_tips}}</p>
  {{/each}}
</div>